import java.util.Scanner;
public class Solution {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        for(int i=n; i>=1; i--){
            for(int j=i; j<=n; j++){
                char temp = (char)('A'+j-1);
                System.out.print(temp);
            }
            System.out.println();
        }
	}
}
